python: 2.7

install:
  - pip install -U nose
  - pip install -U devicewrapper
  - pip install -U customnoseplugins

  --setup.py 

before_script:
  - echo "Before script..."
  - which nosetests

script:
  - nosetests --with-plan-loader --loop 10 --with-device-config --with-local-report --verbosity 2

after_script:
  - echo "after_script"
